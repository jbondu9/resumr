{
  "overrides": [
    {
      "files": ".eslintrc.cjs",
      "options": { "parser": "babel" }
    },
    {
      "files": ["vite.config.ts", "src/**/*.{ts,tsx}"],
      "options": { "parser": "babel-ts" }
    },
    {
      "files": [".prettierrc"],
      "options": { "parser": "json" }
    }
  ],
  "plugins": ["prettier-plugin-tailwindcss"]
}
